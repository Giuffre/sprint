{
  "name": "giuffre/sprint",
  "description": "sprintf with named parameters",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Giuffre\\Sprint\\": "src/"
    },
    "files": [
      "src/Resources/functions/sprint.php"
